      "source": [
        "# General & Visualization Imports\n",
        "import pickle\n",
        "import os\n",
        "import zipfile\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "import warnings\n",
        "\n",
        "# Machine Learning Imports\n",
        "from sklearn.model_selection import train_test_split, cross_val_score, GridSearchCV, StratifiedKFold\n",
        "from sklearn.preprocessing import StandardScaler\n",
        "from sklearn.linear_model import LogisticRegression\n",
        "from sklearn.metrics import (\n",
        "    classification_report, confusion_matrix, f1_score,\n",
        "    precision_score, recall_score, ConfusionMatrixDisplay,\n",
        "    precision_recall_curve, auc\n",
        ")\n",
        "from sklearn.ensemble import RandomForestClassifier\n",
        "from imblearn.pipeline import Pipeline as ImbPipeline\n",
        "from imblearn.over_sampling import SMOTE\n",
        "from xgboost import XGBClassifier\n",
        "import kagglehub\n",
        "\n",
        "# Configuration & Style\n",
        "# Suppress annoying warnings for a cleaner notebook\n",
        "warnings.filterwarnings('ignore')\n",
        "\n",
        "# Set visual style\n",
        "sns.set(style=\"whitegrid\")\n",
        "plt.rcParams['figure.figsize'] = (10, 6)\n",
        "\n",
        "# Ensure pandas shows all columns (crucial for EDA)\n",
        "pd.set_option('display.max_columns', None)\n",
        "\n",
        "# Data Loading\n",
        "print(\"Downloading dataset...\")\n",
        "path = kagglehub.dataset_download(\"umuttuygurr/e-commerce-fraud-detection-dataset\")\n",
        "print(f\"Dataset downloaded to: {path}\")\n",
        "\n",
        "# Identify CSV file\n",
        "# We look for the first .csv file in the directory\n",
        "csv_files = [f for f in os.listdir(path) if f.endswith('.csv')]\n",
        "\n",
        "if not csv_files:\n",
        "    raise FileNotFoundError(f\"No CSV files found in {path}\")\n",
        "\n",
        "# Load the Data\n",
        "file_path = os.path.join(path, csv_files[0])\n",
        "print(f\"Loading {file_path}...\")\n",
        "\n",
        "# Adding parse_dates=['transaction_date'] inside read_csv to indentify this column as a datetime object.\n",
        "df = pd.read_csv(file_path, parse_dates = ['transaction_time'])\n",
